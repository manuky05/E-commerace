package com.BikkadIT.SpringDataJpaFirstApp;

import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.context.ConfigurableApplicationContext;

import com.BikkadIT.SpringDataJpaFirstApp.model.User;
import com.BikkadIT.SpringDataJpaFirstApp.service.serviceImpl;

@SpringBootApplication
public class SpringDataJpaFirstApp1Application {

	public static void main(String[] args) {
		ConfigurableApplicationContext context=SpringApplication.run(SpringDataJpaFirstApp1Application.class, args);
		serviceImpl bean=context.getBean(serviceImpl.class);
		User user =new User();
		user.setUserId(2);
		user.setUseraddress("Saran");
		user.setUserage(25);
		user.setUserName("pawan");
		
		user.setUserId(3);
		user.setUseraddress("Patan");
		user.setUserage(28);
		user.setUserName("Rani");
		
		user.setUserId(4);
		user.setUseraddress("Saran");
		user.setUserage(23);
		user.setUserName("Ranju");
		
		user.setUserId(4);
		user.setUseraddress("Nashik");
		user.setUserage(29);
		user.setUserName("Neeraj");
		
		
		
		
		
		bean.saveUser(user);
	}

}
