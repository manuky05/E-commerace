package collectorOperator;

import java.util.ArrayList;
import java.util.List;
import java.util.stream.Stream;



public class collectorDemo{
	
	public void printEmployeeObject(List<Person> list) {
		
		for (Person l:list) {
			System.out.println(l);
		}
	}
	public static void main(String[] args) {
		Person p1=new Person();
		p1.setName("luliya");
		p1.setAge(22);
		p1.setDesignation("software Enginer");
		
		Person p2=new Person();
		p2.setName("Kalyan");
		p2.setAge(24);
		p2.setDesignation("Doctor");
		
		Person p3=new Person();
		p3.setName("lila");
		p3.setAge(23);
		p3.setDesignation("Layar");
		
		Person p4=new Person();
		p4.setName("shre niwash");
		p4.setAge(30);
		p4.setDesignation("Teacher");
		
		ArrayList<Person> al=new ArrayList();
		
		al.add(p1);
		al.add(p2);
		al.add(p3);
		al.add(p4);
		System.out.println(al);
	Stream<Person> stream=al.stream();
	Stream<Person> filter=stream.filter((p) -> p.getAge() >= 28);
			
		
		
		filter.forEach(System.out::println);
		
		
		
	}

}
